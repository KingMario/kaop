{
  "name": "kaop",
  "version": "2.7.5",
  "description": "simple js OOP/AOP lib helper with classes, inheritance and method Advices (method interceptors)",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "version": "browserify ./index.js -o ./dist/kaop-lib.dev.js ; uglifyjs ./dist/kaop-lib.dev.js --compress --mangle --keep-fnames --output ./dist/kaop-lib.min.js",
    "preversion": "npm test && ./coverage-report && cat coverage/coverage.lcov | ./node_modules/coveralls/bin/coveralls.js",
    "postversion": "git push --follow-tags",
    "report-cover": "./coverage.s"
  },
  "keywords": [
    "framework",
    "helper",
    "oop",
    "aop"
  ],
  "author": "k1r0s",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k1r0s/kaop.git"
  },
  "homepage": "https://github.com/k1r0s/kaop",
  "bugs": {
    "url": "https://github.com/k1r0s/kaop/issues"
  },
  "license": "ISC",
  "devDependencies": {
    "coveralls": "^2.12.0",
    "jscover": "^1.0.0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "uglifyjs": "^2.4.10"
  }
}
